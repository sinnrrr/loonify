  "devDependencies": {
    "@types/brainhubeu__react-carousel": "2.0.2",
    "@types/busboy": "0.2.3",
    "@types/faker": "5.5.1",
    "@types/leaflet": "1.7.0",
    "@types/leaflet-draw": "1.0.3",
    "@types/preview-email": "2.0.0",
    "@types/qrcode": "1.4.0",
