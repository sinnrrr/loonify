    "@types/react": "17.0.3",
    "@types/react-beautiful-dnd": "13.0.0",
    "@types/react-leaflet": "2.8.1",
    "@types/react-leaflet-markercluster": "2.0.0",
    "@types/react-map-gl": "5.2.9",
    "@types/swiper": "5.4.2",
    "eslint": "7.22.0",
    "faker": "5.5.3",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "prettier-plugin-prisma": "0.6.0",
    "pretty-quick": "3.1.0",
    "preview-email": "3.0.4"
